{
	"compilerOptions": {
		"module": "esnext",
		"target": "es2015",
		"jsx": "react-jsx",
		"strict": true,
		"baseUrl": "./app",
		"moduleResolution": "node",
		"paths": {
			"@/*": ["./*"],
			"@api/*": ["api/*"],
			"@assets/*": ["assets/*"],
			"@constants/*": ["constants/*"],
			"@components/*": ["components/*"],
			"@screens/*": ["screens/*"],
			"@store/*": ["store/*"],
			"@styles/*": ["styles/*"],
			"@utils/*": ["utils/*"]
		}
	},
	"exclude": [
		"node_modules",
		"babel.config.js",
		"metro.config.js",
		"jest.config.js"
	]
}
